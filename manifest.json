{
  "name": "No Reddit",
  "description": "Utility for working with the extension tabs api",
  "version": "0.3",
  "permissions": ["tabs", "<all_urls>", "webNavigation", "*://*/*"],
  "background": {
    "scripts": ["jquery-3.2.1.min.js", "background.js"]
  },
  "browser_action": {
    "default_title": "show tab inspector"
  },
  "manifest_version": 2
}
